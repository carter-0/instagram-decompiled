<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="https://schemas.android.com/tools" xmlns:aapt="http://schemas.android.com/aapt">
    <item android:id="@android:id/background">
        <shape android:shape="ring" android:useLevel="false" android:innerRadius="14dp" android:thickness="1.75dp">
            <solid android:color="@color/grey_2"/>
            <size android:height="35dp" android:width="35dp"/>
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <shape android:shape="ring" android:useLevel="true" android:innerRadius="14dp" android:thickness="1.75dp">
            <solid android:color="@color/black"/>
            <size android:height="35dp" android:width="35dp"/>
        </shape>
    </item>
</layer-list>
