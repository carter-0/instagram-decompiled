<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="https://schemas.android.com/tools" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="@color/fds_transparent"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/fds_transparent" android:endColor="@color/black_15_transparent" android:angle="180" android:centerX="0.95" android:centerColor="@color/fds_transparent"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/fds_transparent" android:endColor="@color/black_15_transparent" android:angle="360" android:centerX="0.95" android:centerColor="@color/fds_transparent"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/black_15_transparent" android:endColor="@color/fds_transparent" android:angle="270" android:centerY="0.05" android:centerColor="@color/fds_transparent"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="@color/black_15_transparent" android:endColor="@color/fds_transparent" android:angle="90" android:centerY="0.05" android:centerColor="@color/fds_transparent"/>
        </shape>
    </item>
</layer-list>
