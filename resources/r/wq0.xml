<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:amazon="http://schemas.amazon.com/apk/res/android" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="https://schemas.android.com/tools" xmlns:aapt="http://schemas.android.com/aapt">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="?attr/igds_color_secondary_background"/>
        </shape>
    </item>
    <item android:left="-1dp" android:right="-1dp" android:bottom="-2dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="?attr/igds_color_stroke"/>
        </shape>
    </item>
</layer-list>
